<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true"/>
    <add key="Microsoft.ServiceBus.ConnectionString"
      value="Endpoint=sb://azurerelaytest.servicebus.windows.net/;SharedAccessKeyName=SharedAccessKey;SharedAccessKey=5YEwr1YFY3V7AThi2k/Mldaezr+m6lg5PP5cumlkYUo=;EntityPath=azurerelayservice"/>
  </appSettings>
  <system.web>
